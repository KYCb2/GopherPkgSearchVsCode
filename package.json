{
	"name": "gophersearch",
	"displayName": "GopherPkgSearch",
	"description": "GopherPkgSearch is Extension for easy search Golang packages from pkg.go.dev!",
	"version": "0.0.4",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:gophersearch.searchPkg"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "gophersearch.searchPkg",
				"title": "Search pkg.go.dev"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/request-promise": "^4.1.47",
		"@types/vscode": "^1.56.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"cheerio": "^1.0.0-rc.9",
		"request": "^2.88.2",
		"request-promise": "^4.2.6"
	},
	"publisher": "KYCb",
	"repository": "github.com/KYCb2/GopherPkgSearchVsCode"
}
